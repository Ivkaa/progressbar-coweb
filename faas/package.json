{
  "name": "cowork-progressbar-behind-scene-faas",
  "version": "1.0.0",
  "description": "faas-friendly progressbar cowork system",
  "author": "Matej Nemcek <ybdaba@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "test": "standard",
    "prepackage-functions": "rimraf functions",
    "package-functions": "babel 'functionsES6' --out-dir 'functions' --presets=es2015 --copy-files --ignore 'node_modules'",
    "postpackage-functions": "cd functions && npm i",
    "deploy": "yarn run package-functions && firebase deploy",
    "serve": "firebase serve --only functions"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-latest-node": "^0.4.0",
    "babel-runtime": "^6.26.0",
    "core-js": "^2.5.1",
    "standard": "*"
  },
  "babel": {
    "presets": [
      "es2017"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": "https://github.com/yangwao/progressbar-coweb/"
}
